{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/parking": {
      "get": {
        "summary": "List all registry of the parking",
        "tags": [
          "Parking"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "found"
          }
        }
      },
      "post": {
        "summary": "Creates a registry",
        "tags": [
          "Parking"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "parking",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "plate": {
                  "type": "string"
                }
              },
              "required": [
                "plate"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "created"
          },
          "422": {
            "description": "invalid request"
          }
        }
      }
    },
    "/parking/{id}/pay": {
      "get": {
        "summary": "Pay off parking",
        "tags": [
          "Parking"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "found",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "time": {
                  "type": "string"
                },
                "paid": {
                  "type": "boolean"
                },
                "left": {
                  "type": "boolean"
                }
              },
              "required": [
                "id",
                "time",
                "paid",
                "left"
              ]
            }
          },
          "404": {
            "description": "not found"
          },
          "403": {
            "description": "unsupported accept header"
          }
        }
      }
    },
    "/parking/{id}/out": {
      "get": {
        "summary": "Get out of the parking",
        "tags": [
          "Parking"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "found",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "time": {
                  "type": "string"
                },
                "paid": {
                  "type": "boolean"
                },
                "left": {
                  "type": "boolean"
                }
              },
              "required": [
                "id",
                "time",
                "paid",
                "left"
              ]
            }
          },
          "404": {
            "description": "not found"
          },
          "403": {
            "description": "unsupported accept header"
          }
        }
      }
    },
    "/parking/{plate}": {
      "get": {
        "summary": "History of the plate",
        "tags": [
          "Parking"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "plate",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "found"
          },
          "404": {
            "description": "not found"
          }
        }
      }
    }
  }
}